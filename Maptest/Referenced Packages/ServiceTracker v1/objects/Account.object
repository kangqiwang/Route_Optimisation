alert(&apos;No Attachments for this Client!&apos;); 
} 
else{ 
window.location.href=&apos;/apex/clientAttachmentPage?recID=&apos;+&quot;{!Account.Id}&quot;+&apos;&amp;AccName=&apos;+&quot;{!Account.Name}&quot;+&apos;&amp;serviceId=&apos;+&quot;{!Service__c.Id}&quot;; 
}</url>
