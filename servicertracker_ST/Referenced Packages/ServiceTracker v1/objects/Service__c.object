        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)}; 
var updateRecords = []; 
ScheduleIds = {!GetRecordIds($ObjectType.Service__c)}; 

if(ScheduleIds.length &lt; 1) 
{ alert(&apos;Please select at least one record&apos;) 
} 
else{ 

var answer=confirm(&quot;WARNING: Are you sure you want to update all Open Visits with the Schedule Owner of the selected records?&quot;); 
if (answer) { 

for (var a=0; a&lt;ScheduleIds.length; a++) { //for all records selected

var R=sforce.connection.query(&quot;Select id,STKR__Service_Owner__c from STKR__Service__c where id = &apos;&quot;+ ScheduleIds[a] + &quot;&apos; limit 1&quot;); 

var Resource = R.getArray(&quot;records&quot;); //add Resource

var Vid=sforce.connection.query(&quot;Select Id,STKR__Service__r.STKR__Service_Owner__c,STKR__Service__r.Name from STKR__Visit__c where STKR__Service__c = &apos;&quot;+ ScheduleIds[a] + &quot;&apos; AND STKR__Status__c = &apos;Open&apos; AND STKR__Resource__c != &apos;&quot;+ Resource[0].STKR__Service_Owner__c + &quot;&apos; &quot;); 

var Vrecords = Vid.getArray(&quot;records&quot;); //add all the Service ids to an Array 

//Start Update visits  Script 
if (Vrecords.length &gt; 0) {  
for (var b=0; b&lt;Vrecords.length; b++) { //for all records 
var update_Visit = new sforce.SObject(&quot;STKR__Visit__c&quot;); //create a new sObject for storing updated record details 
update_Visit.Id = Vrecords[b].Id; //set the Id of the selected record 
update_Visit.STKR__Resource__c = Resource[0].STKR__Service_Owner__c; 

updateRecords.push(update_Visit); //add the updated record to our array 

result = sforce.connection.update(updateRecords); //Save the records
update_Visit = []; //Clear the cache
updateRecords = []; //Clear the cache
} //End if no visits
} //End visit Loop 

} //End Service Loop
document.location.reload(true); //refresh the page 

} // End if user clicks Cancel
} // End no records found</url>
