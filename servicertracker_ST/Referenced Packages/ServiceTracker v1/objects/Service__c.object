    <validationRules>
        <fullName>Check_For_Billing_Query</fullName>
        <active>true</active>
        <description>Checks to see if there is a billing query and no information has been inserted</description>
        <errorConditionFormula>AND(
ISPICKVAL(Invoice_Status__c,&apos;Billing Query&apos;),
ISBLANK(Billing_Query__c) 
)</errorConditionFormula>
        <errorDisplayField>Billing_Query__c</errorDisplayField>
        <errorMessage>Please enter Billing Query Information</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Monthly_Frequency</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(
 CONTAINS($Profile.Name,&apos;Admin&apos;), 
 ISPICKVAL(Business_Type__c, &apos;Water&apos;),
 ISPICKVAL(Service_Frequency__c,&apos;Monthly&apos;), 
 ISNULL(Service_Range__c),
 Service_Interval__c &lt;&gt; 1
),
AND(
 CONTAINS($Profile.Name,&apos;Admin&apos;), 
 ISPICKVAL(Service_Frequency__c,&apos;Monthly&apos;), 
 NOT(ISNULL(Service_Range__c)),
 Service_Interval__c &lt;&gt; 1
)
)</errorConditionFormula>
        <errorDisplayField>Service_Interval__c</errorDisplayField>
        <errorMessage>Please ensure the Schedule Interval is set to 1 and you have selected the relevant Months in the Range. This will be treated as a full year schedule.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Contract_and_Account_Status</fullName>
        <active>true</active>
        <description>Checks to see if there is an Active Contract</description>
        <errorConditionFormula>AND(
CONTAINS($Profile.Name,&apos;Admin&apos;), 
ISPICKVAL(Status__c,&apos;Active&apos;), 
 ISPICKVAL(Service_Contract__r.Status__c,&apos;Inactive&apos;)
)</errorConditionFormula>
        <errorMessage>Please update the Service record(s) before making the Contract Inactive.</errorMessage>
    </validationRules>
