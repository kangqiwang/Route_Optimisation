    <fields>
        <fullName>Mileage_Status__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(
(
(Total_Mileage_To_Date__c + (Renewal_Date__c - TODAY()) * (Average_Mileage_per_check__c / 365)) / Contract_Mileage__c) *100
 &gt; 99

, IMAGE(&quot;/img/samples/color_red.gif&quot;,&quot;High&quot;,20,20),

IF(
AND(
(
(Total_Mileage_To_Date__c + (Renewal_Date__c - TODAY()) * (Average_Mileage_per_check__c / 365)) / Contract_Mileage__c) *100 &lt; 100, 
(
(Total_Mileage_To_Date__c + (Renewal_Date__c - TODAY()) * (Average_Mileage_per_check__c / 365)) / Contract_Mileage__c) *100 &gt; 50
), 
IMAGE(&quot;/img/samples/color_red.gif&quot;,&quot;High&quot;,20,20),

IMAGE(&quot;/img/samples/color_green.gif&quot;,&quot;Low&quot;,20,20)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Mileage Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
