    <validationRules>
        <fullName>Check_Account</fullName>
        <active>true</active>
        <errorConditionFormula>Account_Check__c = FALSE</errorConditionFormula>
        <errorDisplayField>Account_lkp__c</errorDisplayField>
        <errorMessage>The Visit Account Must match the Schedule Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Completion_Times</fullName>
        <active>true</active>
        <description>Check to see if the Completion times are accurate</description>
        <errorConditionFormula>AND(
ISPICKVAL(Status__c,&apos;Complete&apos;),
CONTAINS($Profile.Name,&apos;Admin&apos;), 
OR(
ISBLANK(Completion_Time__c),
ISBLANK(Arrival_Time__c),
Completion_Time__c &gt;  NOW() + (1/1440),
Completion_Time__c &lt; Arrival_Time__c
)
)</errorConditionFormula>
        <errorMessage>Please Check The Visit Completion Times</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Visit_Status</fullName>
        <active>true</active>
        <description>Check for an Active Service</description>
        <errorConditionFormula>AND(
CONTAINS($Profile.Name,&apos;Admin&apos;), 
ISPICKVAL(Status__c,&apos;Open&apos;),
ISPICKVAL(Service__r.Status__c,&apos;Inactive&apos;)
)</errorConditionFormula>
        <errorMessage>Please update the Service record before making the record Inactive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Closed</fullName>
        <active>false</active>
        <description>Check if the Visit is closed and there is no Completion or Arrival Times</description>
        <errorConditionFormula>AND(
CONTAINS($Profile.Name,&apos;Admin&apos;), 
Complete__c = TRUE,
OR(
ISBLANK(Completion_Time__c),
ISBLANK(Arrival_Time__c)
)
)</errorConditionFormula>
        <errorMessage>Please enter both an Arrival and Completion time for this visit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Completion_in_the_future</fullName>
        <active>false</active>
        <description>Check that the Completion time is not greater than the current time</description>
        <errorConditionFormula>AND(
CONTAINS($Profile.Name,&apos;Admin&apos;), 
Completion_Time__c &gt; (NOW() + (1/600.0))
)</errorConditionFormula>
        <errorMessage>You cannot close this Visit in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Uncheck_Complete_Flag</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
PRIORVALUE( Complete__c), 
Complete__c = FALSE 
)</errorConditionFormula>
        <errorMessage>This visit has already been completed</errorMessage>
    </validationRules>
