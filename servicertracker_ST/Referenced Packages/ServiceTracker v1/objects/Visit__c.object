        <fullName>Automated_Next_Visit_Date__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>CASE(Service__r.Service_Frequency__c,

&apos;Daily&apos;,Recurrence_Date__c+Service__r.Service_Interval__c,
&apos;Weekly&apos;,Recurrence_Date__c+(Service__r.Service_Interval__c * 7),
&apos;Monthly&apos;,IF(ISNULL(Service__r.Service_Range__c),
ADDMONTHS(Recurrence_Date__c,Service__r.Service_Interval__c),
ADDMONTHS(Due_Date__c,12)),
Due_Date__c +(Service__r.Service_Interval__c * 365.242)

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
