        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} //adds the proper code for inclusion of AJAX toolkit 
var url = parent.location.href; //string for the URL of the current page 
var records = {!GETRECORDIDS($ObjectType.Visit__c)}; //grabs the Visit records that the user is requesting to update 
var updateRecords = []; //array for holding records that this code will ultimately update 

if (records[0] == null) { //if the button was clicked but there was no record selected 
alert(&quot;Please select at least one record to update.&quot;); //alert the user that they didn&apos;t make a selection 
} else { //otherwise, there was a record selection 
var answer=confirm(&quot;WARNING: This will update all selected records and schedule visits based on Territory and Skill. Are you sure you want to proceed?&quot;);
if (answer) { // If User Clicks OK

var update_Visit= new sforce.SObject(&quot;STKR__Visit__c&quot;); //create a new sObject for storing updated record details 

for (var a=0; a&lt;records.length; a++) { //for all records 

var ter = sforce.connection.query(&quot;Select STKR__Territory__c,STKR__Skill_Required__c from STKR__Visit__c Where Id = &apos;&quot; + records[a] + &quot;&apos;&quot;);
var resource = ter.getArray(&apos;records&apos;);

if( resource[a] != null){

var res = sforce.connection.query(&quot;Select Id from STKR__Resource__c where STKR__Territory__c = &apos;&quot; + resource[a].STKR__Territory__c + &quot;&apos; AND Id in (Select STKR__Resource__c from STKR__Resource_Skill__c where STKR__Skill_Name__c = &apos;&quot; + resource[a].STKR__Skill_Required__c + &quot;&apos;)&quot;);

var resource = res.getArray(&quot;records&quot;);

if (resource[a] == null) { //if there are no resources
alert(&quot;There are no resources for this visit&quot;); //alert the user 
} else {
update_Visit.Id = records[a]; //set the Id of the selected record 
update_Visit.STKR__Resource__c = resource[a].Id; //Close the tasks 
updateRecords.push(update_Visit); //add the updated record to our array 
resource = &quot;&quot;;

} 
result = sforce.connection.update(updateRecords); //push the updated records back to Salesforce 
}
parent.location.href = url; //refresh the page 
}
}
}</url>
