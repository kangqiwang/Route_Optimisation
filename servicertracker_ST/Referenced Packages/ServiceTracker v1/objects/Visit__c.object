        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/23.0/connection.js&quot;)}
var url = parent.location.href; 
var updateRecord; 
var vis = sforce.connection.retrieve(&quot;Id,STKR__Due_Date__c,STKR__Due_Finish__c&quot;, &quot;STKR__Visit__c&quot;, [&apos;{!Visit__c.Id }&apos;]);
var n = new Date().toISOString();
var update_visit = new sforce.SObject(&quot;STKR__Visit__c&quot;);
var iRecord = new sforce.SObject(&quot;STKR__Inspection__c&quot;); 
var idate = new Date().toISOString();

update_visit.Id = &apos;{!Visit__c.Id}&apos;;
if(vis[0].STKR__Due_Finish__c &lt; n){
update_visit.STKR__Arrival_Time__c = vis[0].STKR__Due_Date__c;
update_visit.STKR__Completion_time__c = vis[0].STKR__Due_Finish__c;
idate = vis[0].STKR__Due_Date__c;
} else {
update_visit.STKR__Arrival_Time__c = n;
update_visit.STKR__Completion_time__c = n;
}
update_visit.STKR__Status__c = &quot;Complete&quot;;

var updateInspections = confirm(&quot;This will also update all Inspections on this Visit? Click OK to continue or Cancel to update the visit only&quot;);

if (updateInspections){

var strQuery=&quot;Select Id from STKR__Inspection__c where STKR__Visit__c = &apos;&quot; +&apos;{!Visit__c.Id}&apos;+&quot;&apos;&quot;;

var inspections = sforce.connection.query(strQuery);
var nrecords = inspections.getArray(&quot;records&quot;);
for(var i=0; i &lt; nrecords.length ; i ++)
{
        iRecord.Id = nrecords[i].Id;
        iRecord.STKR__Status__c = &quot;Inspected&quot;;
        iRecord.STKR__Inspection_Date_Time__c = idate;
        sforce.connection.update([iRecord]);

}

}
sforce.connection.update([update_visit]);

parent.location.href = url;</url>
