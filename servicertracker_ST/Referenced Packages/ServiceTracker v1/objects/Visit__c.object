} 
} 
if (numFailed &gt; 0){ 
alert(&quot;Failed: &quot; + numFailed + &quot;\nSucceeded: &quot; + numSucceeded + &quot; \n Due to: &quot; + errors.join(&quot;\n&quot;)); 
} else { 
alert(&quot;Number of records deleted: &quot; + numSucceeded); 
} 
} 
window.location.reload(); 
}</url>
